        INCLUDES = -I. -I$(INCLUDESRC) -I$(MINCLUDESRC) AutoIncludes \
		   -I../../../lib/
        DEPLIBS1 = XmClientDepLibs
 LOCAL_LIBRARIES = XmClientLibs

            OBJS = debug.o disp.o free.o pack.o recv.o send.o util.o utm_send.o
            SRCS = debug.c disp.c free.c pack.c recv.c send.c util.c utm_send.c

HEADERS = wsm_funcs.h	wsm_proto.h

all::

.c.o:
	$(RM) $@
	 $(CC) -c $(CFLAGS) $(_NOOP_) $*.c

all:: libWsm.a

libWsm.a: $(OBJS) $(EXTRALIBRARYDEPS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(_NULLCMD_)
	$(_NULLCMD_)

includes:: $(HEADERS)
	@if [ -d $(BUILDINCDIR)/Wsm ]; then \
	set +x; \
	else \
	if [ -h $(BUILDINCDIR)/Wsm ]; then \
	(set -x; rm -f $(BUILDINCDIR)/Wsm); \
	fi; \
	(set -x; $(MKDIRHIER) $(BUILDINCDIR)/Wsm); \
	fi
	@(set -x; cd $(BUILDINCDIR)/Wsm && for i in $(HEADERS); do \
	$(RM) $$i; \
	$(LN) $(BUILDINCTOP)/../$(CURRENT_DIR)/$$i .; \
	done)

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	$(RM) $@
	 $(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@

.SUFFIXES: .ii

.cc.ii:
	$(RM) $@
	 $(CC) -E $(CFLAGS) $(_NOOP_) $*.cc > $@

.SUFFIXES: .s

.c.s:
	$(RM) $@
	 $(CC) -S $(CFLAGS) $(_NOOP_) $*.c

.cc.s:
	$(RM) $@
	 $(CC) -S $(CFLAGS) $(_NOOP_) $*.cc

emptyrule::

cleandir::
	$(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut   "#"*

Makefile::
	-@if [ -f Makefile ]; then set -x; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

man_keywords::
	catman -M $(DESTDIR)$(MANPATH) -w

html_index::

clean:: cleandir

distclean:: cleandir

